<Window x:Class="lpubsppop01.AnyTextFilterVSIX.AboutThisSoftwareWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:lpubsppop01.AnyTextFilterVSIX"
        Title="{local:NameToResourceBinding AboutThisSoftware}" Height="600" Width="900">
    <DockPanel Margin="6">
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,6,0,0">
            <Button Name="btnClose" Click="btnClose_Click" IsDefault="True" IsCancel="True" Width="70" Content="{local:NameToResourceBinding Close}"/>
        </StackPanel>
        <WebBrowser Name="ctrlMain"/>
    </DockPanel>
</Window>
